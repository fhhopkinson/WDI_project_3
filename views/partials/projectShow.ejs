<section id="projectShow">
  <div class="pure-g topbar-showproject">
    <div class="pure-u-6-24">

    </div>
    <div class="pure-u-1-24">

    </div>
    <div class="pure-u-11-24">
      <h1 id="showTitle">Project Show Page </h1>
    </div>
    <div class="pure-u-2-24">
      <div id="avatarBox">

      </div>
    </div>
  </div>
  <div class="pure-g">
    <div class="pure-u-6-24">
      <h4>Are you attending?</h4>
      <button class="button-large pure-button" id="eventAttendingYes">Yes</button>
      <button class="button-large pure-button">No</button>
      <ul id="attendeesList">
      </ul>
    </div>
    <div class="pure-u-1-24">

    </div>
    <div class="pure-u-11-24">
      <div id="showImage">
      </div>
      <p id="showDesc">
      </p>  
      <p id="showAddress">
      </p>

      <form class="pure-form pure-form-stacked" id="comment" action="/projects" method="PUT">
        <fieldset>
          <legend>Add a new comment</legend>
          <br>
          <label for="commenter">Name</label>
          <input id="name" name="commenter" type="text"></br>
          <label for="comment">Comment</label>
          <textarea id="comment" name="comment" placeholder="Your comment here"> </textarea>
          <button type="submit" class="pure-button pure-button-primary">Add Comment</button>
        </fieldset>
      </form>
      Comments:
      <ul id="commentsList">

      </ul>
    </div>
    <div class="pure-u-2-24">
      <div id="imageFound">Image From Google Places</div>
      <div id="mapSmallProjectShow">
       Small Map goes here
     </div>
   </div>

 </div>
</section>