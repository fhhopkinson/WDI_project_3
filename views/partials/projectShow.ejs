<section id="projectShow">
  <div class="pure-g topbar-showproject">
    <div class="pure-u-7-24" >

    </div>
  
    <div class="pure-u-11-24">
      <h1 id="showTitle">Project Show Page </h1>
    </div>
    <div class="pure-u-2-24">
      <div id="avatarBox">

      </div>
    </div>
  </div>
  <div class="pure-g middleProjectShow">
    <div class="pure-u-6-24">
      <h4>Are you attending?</h4>
